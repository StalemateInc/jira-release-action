{
  "name": "jira-release-action",
  "version": "1.1.0",
  "private": true,
  "description": "Jira Release action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src/**/*.ts",
    "format-check": "prettier --check src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --minify --no-source-map-register --license licenses.txt",
    "test": "jest --passWithNoTests",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StalemateInc/jira-release-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Nicolas Charpentier, Yahor Dounar",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^16.18.38",
    "@typescript-eslint/parser": "5.62.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.44.0",
    "eslint-plugin-github": "^4.8.0",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-prettier": "^5.0.0-alpha.2",
    "jest": "^29.6.1",
    "prettier": "3.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
