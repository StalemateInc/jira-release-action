{
  "name": "jira-release-action",
  "version": "1.2.1",
  "private": true,
  "description": "Jira Release action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src/**/*.ts",
    "format-check": "prettier --check src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --minify --no-source-map-register --license licenses.txt",
    "test": "jest --passWithNoTests",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StalemateInc/jira-release-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Nicolas Charpentier, Yahor Dounar",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "^4.10.2",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  },
  "volta": {
    "node": "20.18.1"
  }
}
